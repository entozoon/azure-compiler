{
	"from": "../../",
	"css": [
		{
			"watch": [
				"src/scss/**/*.css",
				"src/scss/**/*.scss"
			],
			"src": "src/scss/app.scss",
			"dest": "./dist/css",
			"filename": "app.css",
			"modes": {
			},
			"cleanCssOptions": {
			}
		}
	],
	"js": [
		{
			"watch": [
				"src/js/**/*.js"
			],
			"src": "src/js/app.js",
			"dest": "./dist/js",
			"filename": "app.js",
			"modes": {
			}
		}
	]
}
